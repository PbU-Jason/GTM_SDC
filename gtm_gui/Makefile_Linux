.c.o:
	gcc -fPIC -c $<

all: GTM_Decoder_Function.o GTM_Decoder_Parse_Science_Data.o GTM_Decoder_Parse_TMTC_Data.o GTM_Decoder_Main.o
	gcc -shared GTM_Decoder_Function.o GTM_Decoder_Parse_Science_Data.o GTM_Decoder_Parse_TMTC_Data.o -o libheader.so
	gcc -shared GTM_Decoder_Main.o -Wl,-rpath=. -L . -lheader -o GTM_Decoder_Main.so
